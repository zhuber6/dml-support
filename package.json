{
  "name": "dml-support",
  "displayName": "dml-support",
  "description": "DML language support for Windriver's Simics",
  "version": "1.0.0",
  "publisher": "zhuber6",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "dml",
      "aliases": ["Device Modeling Language", "dml"],
      "extensions": [".dml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "dml",
      "scopeName": "source.dml",
      "path": "./syntaxes/dml.tmLanguage.json"
    }]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zhuber6/dml-support.git"
  }
}
